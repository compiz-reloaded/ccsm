#, python-format
msgid ""
"The new value for the %(binding)s binding for the action <b>%(action)s</b> "
"in plugin <b>%(plugin)s</b> conflicts with the action <b>%(action_conflict)"
"s</b> of the <b>%(plugin_conflict)s</b> plugin.\n"
"Do you wish to disable <b>%(action_conflict)s</b> in the <b>%"
"(plugin_conflict)s</b> plugin?"
msgstr ""
"O novo valor de %(binding)s para a ação <b>%(action)s</b> no engato <b>%"
"(plugin)s</b> está em conflito com a ação <b>%(action_conflict)s</b> do "
"engato <b>%(plugin_conflict)s</b>.\n"
"Voce queira deshabilitar <b>%(action_conflict)s</b> no engato <b>%"
"(plugin_conflict)s</b> ?"

#, python-format
msgid "Disable %(action_conflict)s"
msgstr "Deshabilita %(action_conflict)s !"

#, python-format
msgid "Don't set %(action)s"
msgstr "Não atribua valor a %(action)s !"

#, python-format
msgid "Set %(action)s anyway"
msgstr "Atribua um valor a %(action)s de qualquer forma !"

#, python-format
msgid ""
"You are trying to use the feature <b>%(feature)s</b> which is provided by <b>"
"%(plugin)s</b>.\n"
"This plugin is currently disabled.\n"
"Do you wish to enable <b>%(plugin)s</b> so the feature is available?"
msgstr ""
"Voce está tentando a usar à função <b>%(feature)s</b> que é providenciado "
"por <b>%(plugin)s</b>.\n"
"Este engato atualmente está deshabilitado.\n"
"Voce queria habilitar <b>%(plugin)s</b> de modo que possa ser usada aquela "
"função?"

#, python-format
msgid "Enable %(plugin)s"
msgstr "habilita engato %(plugin)s !"

#, python-format
msgid "Don't enable %(feature)s"
msgstr "Não habilita %(feature)s !"

#, python-format
msgid ""
"Plugin <b>%(plugin_conflict)s</b> provides feature <b>%(feature)s</b> which "
"is also provided by <b>%(plugin)s</b>"
msgstr ""
"Conflito:  Engato <b>%(plugin_conflict)s</b> providencia a função <b>%"
"(feature)s</b> a qual também esta sendo providenciado pelo engato <b>%"
"(plugin)s</b>"

#, python-format
msgid "Disable %(plugin_conflict)s"
msgstr "deshabilita engato  %(plugin_conflict)s !"

#, python-format
msgid "Don't enable %(plugin)s"
msgstr "Não habilita %(plugin)s !"

#, python-format
msgid "Plugin <b>%(plugin_conflict)s</b> conflicts with <b>%(plugin)s</b>."
msgstr "Gato <b>%(plugin_conflict)s</b> conflita com <b>%(plugin)s</b>."

#, python-format
msgid ""
"<b>%(plugin)s</b> requires feature <b>%(feature)s</b> which is provided by "
"the following plugins:\n"
"%(plugin_list)s"
msgstr ""
"<b>%(plugin)s</b> necessita da função <b>%(feature)s</b> a qual é "
"providenciada pelos engatos:\n"
"%(plugin_list)s"

msgid "Enable these plugins"
msgstr "Habilita estes gatos !"

#, python-format
msgid "<b>%(plugin)s</b> requires the plugin <b>%(require)s</b>."
msgstr "<b>%(plugin)s</b> necessita do engato <b>%(require)s</b>."

#, python-format
msgid "Enable %(require)s"
msgstr "Habilita %(require)s !"

#, python-format
msgid ""
"<b>%(plugin)s</b> provides the feature <b>%(feature)s</b> which is required "
"by the plugins <b>%(plugin_list)s</b>."
msgstr ""
"<b>%(plugin)s</b> providencia à função <b>%(feature)s</b> a qual é "
"necessária para os entagos <b>%(plugin_list)s</b>."

msgid "Disable these plugins"
msgstr "Deshabilita estes gatos !"

#, python-format
msgid "Don't disable %(plugin)s"
msgstr "Não deshabilita %(plugin)s"

#, python-format
msgid "<b>%(plugin)s</b> is required by the plugins <b>%(plugin_list)s</b>."
msgstr "<b>%(plugin)s</b> é necessário para os engatos <b>%(plugin_list)s</b>."

